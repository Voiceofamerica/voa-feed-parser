{
  "name": "voa-feed-parser",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf .build && tsc",
    "build:test": "tsc -p tests",
    "pretest": "npm run build:test",
    "test": "JASMINE_CONFIG_PATH=./jasmine.json jasmine",
    "lint": "tslint -p .",
    "posttest": "rimraf tests/.build",
    "standardize": "prettier *.ts ./src/*.ts ./src/**/*.ts --write",
    "predeploy": "npm run standardize && npm run lint && npm test",
    "deploy": "serverless deploy -v",
    "deploy:production": "serverless deploy -v --stage prod",
    "logs": "serverless logs -f articles -t",
    "invoke": "serverless invoke -f articles -l",
    "offline": "serverless offline --port 3007 start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Voiceofamerica/voa-mobile-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Voiceofamerica/voa-mobile-server/issues"
  },
  "homepage": "https://github.com/Voiceofamerica/voa-mobile-server#readme",
  "dependencies": {
    "@voiceofamerica/voa-core-shared": "^1.25.0",
    "bluebird": "^3.5.1",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.21",
    "@types/jasmine": "^2.6.2",
    "@types/node": "^8.0.47",
    "@types/request-promise-native": "^1.0.10",
    "jasmine": "^2.8.0",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2",
    "serverless": "^1.24.0",
    "serverless-offline": "^3.16.0",
    "serverless-plugin-typescript": "^1.1.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  },
  "prettier": {
    "printWidth": 90,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "parser": "typescript"
  }
}
